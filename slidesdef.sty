\newcommand{\makesection}[2]{
  \iflanguage{portuguese}{
    \section[#1]{#1}
  }{
    \section[#2]{#2}
  }
}
\newcommand{\makesubsection}[2]{
  \iflanguage{portuguese}{
    \subsection[#1]{#1}
  }{
    \subsection[#2]{#2}
  }
}
\newcommand{\myframe}[1]{
